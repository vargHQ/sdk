    "@types/react": "^19.2.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7"
  },
  "peerDependencies": {
    "typescript": "^5",
    "ai": "^6.0.0",
    "zod": "^3.25.76 || ^4.1.8"
  },
  "dependencies": {
    "@ai-sdk/fal": "^1.0.23",
    "@ai-sdk/fireworks": "^2.0.16",
    "@ai-sdk/groq": "^3.0.12",
